{
  "name": "storystore_client",
  "version": "1.0.0",
  "description": "Client-side for Storystore",
  "author": "Adiv Ohayon",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "cross-env NODE_ENV=production nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
		"lintFix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "server": "npm run build && node server/server.js",
    "deploy:sls": "npm run build && sls deploy --region eu-central-1",
    "deploy:static": "aws s3 cp ./client/static/ s3://cdn.shop-together.io --recursive --exclude \".DS_Store\"",
    "sls:offline": "npm run build && sls offline start",
    "svg": "vsvg -s ./svgs -t ./client/icons --es6"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "googleapis": "^36.0.0",
    "lodash.get": "^4.4.2",
    "mobile-detect": "^1.4.3",
    "nodemailer": "^5.1.1",
    "nuxt": "^2.3.1",
    "nuxt-sass-resources-loader": "^2.0.5",
    "scroll-snap": "^1.0.6",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-http": "^1.8.0",
    "swiper": "^4.4.6",
    "v-selectmenu": "^1.0.7",
    "vue-fullpage.js": "^0.1.1",
    "vue-scrollto": "^2.13.0",
    "vue-video-player": "^5.0.2",
    "vue2-touch-events": "^1.1.1",
    "vuex-persist": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "event-stream": "^3.3.4",
    "node-sass": "^4.10.0",
    "nodemon": "^1.11.0",
    "prettier": "1.14.3",
    "sass-loader": "^7.1.0",
    "serverless-domain-manager": "^2.6.6",
    "serverless-offline": "^3.31.3",
    "vue-svgicon": "^3.2.2"
  }
}
